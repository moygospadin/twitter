  "scripts": {
    "build:twitter": "nest build twitter",
    "build:chat": "nest build twitter-chat",
    "build": "run-p build:*",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "start:twitter": "nest start twitter --watch",
    "start:chat": "nest start twitter-chat --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test:e2e": "jest --config ./jest.config.js",
    "prisma:generate": "bash ./prisma/generate-client.sh twitter TWITTER_POSTGRES_URL",
    "migration:up": "cd migrations/twitter && sqitch deploy",
    "migration:revert": "cd migrations/twitter && sqitch revert -y"
  },
